<div class="container">
  <div class="row">
    <div class="col-8">
      <% @posts.each do |post|%>
        <div class="insta-post">
           <div class="post-meta">
              <%=image_tag (post.account.image.variant(resize: "100x100")%>
              <%= post.account.username %>
           </div>
          <%= image_tag (post.image), class:"img img-fluid" %>
        </div>
      <%end%>
    </div>
    <div class="col-4">
      <div class="account-summery">
        <p>
          <strong> <%=current_account.username%></strong>
          </br>
            <%=current_account.full_name%>
        </p>
      </div>
      <div class="follower-suggestions">
      </div>
    </div>
  </div>
</div>
