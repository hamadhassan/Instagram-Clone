<div class="container">
  <div class="row">
    <div class="col-8">
      <% @posts.each do |post|%>
        <div class="insta-post">
           <div class="post-meta">
              <%= post.account.username %>
           </div>
          <%= image_tag (post.image), class:"img img-fluid" %>
        </div>
      <%end%>
    </div>
    <div class="col-4">
      <div class="account-summery">
        <p> <stronge> </p>
        <%=current_account.username%> </br>
        <%=current_account.full_name%>
      </div>
    </div>
  </div>
</div>
